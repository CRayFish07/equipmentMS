<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.moutum.equ.domain">

	<class name="DatumType" table="equms_datumtype_info">
		<id name="datumTypeId" column="datumtype_id">
			<generator class="native" />
		</id>
		<property name="datumTypeName" column="datumtype_name" not-null="true"/>
		<property name="remark" column="remark"/>
	</class>

</hibernate-mapping>