<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.moutum.equ.domain">

	<class name="Modle" table="equms_model_info">
		<id name="modleId" column="modle_id">
			<generator class="native" />
		</id>
		<property name="name" column="name" not-null="true"/>
		<property name="parentId" column="parent_id" not-null="true"/>
		<property name="url" column="url" not-null="true"/>
	</class>

</hibernate-mapping>