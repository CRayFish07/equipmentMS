<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.moutum.equ.domain">

	<class name="Right" table="equms_right_info">
		<id name="rightId" column="right_id">
			<generator class="native" />
		</id>
		<property name="name" column="name" not-null="true"/>
		<property name="functionName" column="function_name" not-null="true"/>
		<property name="modleId" column="modle_id" not-null="true"/>
		<property name="remark" column="remark"/>
	</class>

</hibernate-mapping>